<% if @ptnssp_list.present? %>

<h2> SENARAI CADANGAN AJK </h2>

<div class="card mb-3">
  <div class="card-header">
    <i class="fas fa-eye"></i>
    DISPLAYING ALL INPUT
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-striped table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th> NO </th>
            <th> NAME </th>
            <th> STRENGTH </th>
            <th> WEAKNESS </th>
            <th> OPPORTUNITY </th>
            <th> THREAT </th>
            <th> ACTION </th>
          </tr>
        </thead>
        <tbody>
          <% counter = 1 %>
          <% @ptnssp_list.each do |item|%>
            <tr>
              <td><%= counter %></td>
              <td width="250px"><%= item.name %></td>
              <td><%= item.strgh %></td>
              <td><%= item.wkns %></td>
              <td><%= item.opp %></td>
              <td><%= item.thr %></td>
              <td><%= link_to "EDIT", edit_ptnssp_path(item), class: 'btn btn-sm btn-success active' %></td>
            </tr>
          <% counter = counter +1 %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<% else %>

No input receive yet

<% end %>