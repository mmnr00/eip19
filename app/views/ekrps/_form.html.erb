                <b>Catatan :</b><br>
                <%= f.text_area :ci, rows: 5, cols: 60%><br>
                <b>Keseimbangan : </b><%= f.select :cj, options_for_select(["Ya", "Tidak"],selected: @ekrp.cj),{include_blank: "Pilih Satu"}, {required: true} %><br>
                <b>Catatan :</b><br>
                <%= f.text_area :ck, rows: 5, cols: 60%><br>
              </td>
            </tr>
          </tbody>     
        </table>
      </div>

      <!-- TINGKAHLAKU/SENSORI -->
      <div class="table-responsive">
        <table class="table table-bordered" >
          <thead style="color: white">
