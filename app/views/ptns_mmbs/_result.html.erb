<%= render partial: "shared/flash_messages", flash: flash %>
  <% if @ptns_find.present? %>
    <div class="table-responsive">
      <table class="table table-striped table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr style="background-color:#d8b2d8">
            <th> NAMA </th>
            <th> TINDAKAN </th>
          </tr>
        </thead>
        <tbody> 
          <% @ptns_find.each do |mmb| %>
            <tr>
              <td><%= mmb.name %></td>
              <td>
                <%= link_to edit_ptns_mmb_path(id: mmb.id, type: mmb.tp), class: "btn btn-sm active", style: "color:green" do %>
                    <i class="fa fa-edit"></i> UBAH
                <% end %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  <% end %> 

  




