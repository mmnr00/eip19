NEW TCHDETAILS

<div class='row'>
    <div class='col-lg-12'>
      <%= form_for(@tchdetail, :html => {class: "form-horizontal", role: "form"}) do |f| %>
        
        <div class="form-group"> <!-- name -->
          <div class="control-label col-sm-2">
            <%= f.label :name %>
          </div>
          <div class="col-sm-8">
            <%= f.text_field :name, class: "form-control", placeholder: "Children Name", autofocus: true %>
          </div>
        </div>

        <div class="form-group"> <!-- IC -->
          <div class="control-label col-sm-2">
            <%= f.label :IC %>
          </div>
          <div class="col-sm-8">
            <%= f.text_field :ic_1, class: "form-control", placeholder: "Children Name", autofocus: true %> - 
            <%= f.text_field :ic_2, class: "form-control", placeholder: "Children Name", autofocus: true %> -
            <%= f.text_field :ic_3, class: "form-control", placeholder: "Children Name", autofocus: true %>
          </div>
        </div>

        <div class="form-group"> <!-- PHONE-->
          <div class="control-label col-sm-2">
            <%= f.label :phone %>
          </div>
          <div class="row">
            <div class="col-sm-2">
            <%= f.text_field :phone_1, class: "form-control", placeholder: "Children Name", autofocus: true %> 
            </div>
            -
            <div class="col-sm-2">
            <%= f.text_field :phone_2, class: "form-control", placeholder: "Children Name", autofocus: true %>
          </div>
          </div>
        </div>

          
          
          <div class="col-md-4">
                  <%= f.hidden_field :parent_id, value: params[:id] %>
              </div>
              
        <div class="form-group">
          <div class="col-sm-offset-2 col-sm-10">
            <%= f.submit class: 'btn btn-primary btn-lg' %>
          </div>
        </div>
      <% end %>
      
    </div>
  </div>