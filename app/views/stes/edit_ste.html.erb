<h4>SPEECH THERAPIST ASSESSMENT FOR</h4> 
<h3><%= @ste.ekid.name.upcase %></h3>
<br>
<div class='col-12'>
  <%= form_for(:ste,:url => {:controller => "stes", :action => "upd_ste"}, :html => {multipart: true, class: "form-horizontal", role: "form"}) do |f| %>

    <%= f.fields_for :chkl do |g| %>
      <%= render 'chkls/formchkl', g: g%>
    <% end %>

    <%= render 'stes/form_ste', f: f%>
    
    <%= f.submit :"UPDATE", class: 'btn btn-success btn-m btn-block'%>

  <% end %>
</div>