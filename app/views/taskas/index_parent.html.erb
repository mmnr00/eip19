<div class="card mb-3">
  <div class="card-header">
    <i class="fas fa-search"></i>
    VIEWING AVAILABLE CENTRE
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-striped table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th> N0 </th>
            <th> NAME </th>
            <th> ADDRESS </th>
            <th> SUPERVISOR </th>
            <th> CONTACT NO </th>
          </tr>
        </thead>
        <tbody>
          <% counter = 1 %>
          <% @taskas.each do |taska|%>
            <tr>
              <td><%= counter %></td>
              <td><%= taska.name %></td>
              <td>
                <%= taska.address_1 %></br>
                <%= taska.address_2 %></br>
                <%= taska.city %>&nbsp
                <%= taska.states %>&nbsp
                <%= taska.postcode %>
              </td>
              <td><%= taska.email %></td>
              <td>
                <% if params[:kid_id].present? %>
                  <%= link_to "CHOOSE", add_taska_path(kid_id: params[:kid_id], taska_id: taska.id, parent_id: params[:id]),method: :post, class: "btn btn-sm btn-info active" %>
                <% else %>
                  <%= link_to "CHOOSE", new_kid_path(@parent.id, taska_id: taska.id), class: "btn btn-sm btn-info active" %>
                <% end %>
              </td>
            </tr>
          <% counter = counter + 1 %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<%= render 'js-script/datatables-demo'%> 