<%= render 'layouts/dsb-admin-head' %>


      <!-- Sidebar -->
      <ul class="sidebar navbar-nav">
        <li class="nav-item active">
          <a class="nav-link" href="/taskas/<%= @taska.id %>">
            <i class="fas fa-fw fa-tachometer-alt"></i>
            <span>Overview</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/taska/<%= @taska.id %>/classroom">
            <i class="fas fa-fw fa-child"></i>
            <span>My Classrooms </span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">
            <i class="fas fa-fw fa-chart-bar"></i>
            <span>My Accounts <font size="2">(pending)</font></span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">
            <i class="fas fa-fw fa-star"></i>
            <span>My Ratings <font size="2">(pending)</font></span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">
            <i class="fas fa-fw fa-chalkboard-teacher"></i>
            <span>My Teachers <font size="2">(pending)</font></span>
          </a>
        </li>
      </ul>

      <div id="content-wrapper">
        <%= render 'layouts/admin_db/paynow' %>
        <div class="container-fluid">
          <ol class="breadcrumb">
            <li class="breadcrumb-item">
              Admin Dashboard
            </li>
            <li class="breadcrumb-item active">Overview for <strong><%= @taska.name.upcase %></strong></li>
          </ol>
          <div class="bg-light">
            <div class="container">
              <div class="srow">
              <%= render partial: "shared/flash_messages", flash: flash %>
                <%= yield %>
              </div>
            </div>
          </div>
        </div>
      </div>

<%= render 'layouts/dsb-admin-foot' %>


        
          

          
