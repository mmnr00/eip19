<div class="table-responsive">
  <table class="table table-striped table-bordered" width="100%" cellspacing="0">
    <tbody>
      <tr>
        <td style="background-color:#ffedcc; text-align:center; font-size:20px" colspan="2">
          <b>ASSESSMENTS CHECKLIST</b>
        </td>
      </tr>

      <!-- GROSS MOTOR -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>GROSS MOTOR</b>
        </td>
      </tr>
      <% @chkl.gmtr.each do |k,v| %>
        <tr>
          <td><%= k %></td>
          <td><%= v %></td>
        </tr>
      <% end %>

      <!-- FINE MOTOR -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>FINE MOTOR</b>
        </td>
      </tr>
      <% @chkl.fmtr.keys[0..7].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.fmtr[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td>COPYING 'O' 'SQUARE' /' '\' '-'</td>
        <td><%= @chkl.fmtr["COPYING"] %></td>
      </tr>
      <% @chkl.fmtr.keys[9..100].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.fmtr[k] %></td>
        </tr>
      <% end %>

      <!-- ADL & BEHAVIOUR -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>ADL & BEHAVIOUR</b>
        </td>
      </tr>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>ADL</u>
        </td>
      </tr>
      <% @chkl.adbh.keys[0..4].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.adbh[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>BEHAVIOUR</u>
        </td>
      </tr>
      <% @chkl.adbh.keys[5..100].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.adbh[k] %></td>
        </tr>
      <% end %>

      <!-- SPEECH & LANGUAGE -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>SPEECH & LANGUAGE</b>
        </td>
      </tr>
      <% @chkl.splg.keys[0..1].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.splg[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>PREVERBAL SKILLS</u>
        </td>
      </tr>
      <% @chkl.splg.keys[2..7].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.splg[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>RECEPTIVE SKILLS</u>
        </td>
      </tr>
      <% @chkl.splg.keys[8..9].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.splg[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td colspan="2"><b>-PICTURE IDENTIFICATION-</b></td>   
      </tr>
      <% @chkl.splg.keys[10..11].each do |k| %>
        <tr>
          <td><%= "-#{k}" %></td>
          <td><%= @chkl.splg[k] %></td>
        </tr>
      <% end %>
      <% @chkl.splg.keys[12..15].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.splg[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>EXPRESSIVE SKILLS</u>
        </td>
      </tr>
      <% @chkl.splg.keys[15..100].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.splg[k] %></td>
        </tr>
      <% end %>

      <!-- COGNITIVE -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>COGNITIVE</b>
        </td>
      </tr>
      <% @chkl.cogn.keys[0..12].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.cogn[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td colspan="2"><b>-SIZE CONCEPT-</b></td>   
      </tr>
      <% @chkl.cogn.keys[13..15].each do |k| %>
        <tr>
          <td><%= "-#{k}" %></td>
          <td><%= @chkl.cogn[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td>DEVELOPMENT AGE</td>
        <td><%= @chkl.cogn["DEVELOPMENT AGE"] %></td>
      </tr>

      <!-- AUDIOLOGIST -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>AUDIOLOGIST</b>
        </td>
      </tr>
      <% @chkl.audl.each do |k,v| %>
        <tr>
          <% if k == "DIFFICULT SITUATION TO HEAR" %>
            <td><%= "#{k} (noisy/quiet/tv/kindergarden/etc)" %></td>
          <% else %>
            <td><%= k %></td>
          <% end %>
          <td><%= v %></td>
        </tr>
      <% end %>

      <!-- PREVERBAL SKILLS -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>PREVERBAL SKILLS</b>
        </td>
      </tr>
      <% @chkl.prvb.keys[0..3].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.prvb[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>MATCHING</u>
        </td>
      </tr>
      <% @chkl.prvb.keys[4..100].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.prvb[k] %></td>
        </tr>
      <% end %>

      <!-- RECEPTIVE & EXPRESSIVE LANGUAGE SKILLS -->
      <tr>
        <td style="background-color:#d8b2d8; text-align:center; font-size:17px" colspan="2">
          <b>RECEPTIVE & EXPRESSIVE LANGUAGE SKILLS</b>
        </td>
      </tr>
      <tr>
        <td>IDENTIFY/PRODUCE NOUN</td>
        <td><%= @chkl.rcel["NOUN"] %></td>
      </tr>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>IDENTIFY/PRODUCE VERB</u>
        </td>
      </tr>
      <% @chkl.rcel.keys[1..5].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.rcel[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>IDENTIFY/PRODUCE BODY PARTS</u>
        </td>
      </tr>
      <% @chkl.rcel.keys[6..12].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.rcel[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>IDENTIFY/PRODUCE COLOUR</u>
        </td>
      </tr>
      <% @chkl.rcel.keys[13..17].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.rcel[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>IDENTIFY/PRODUCE SIZE CONCEPT</u>
        </td>
      </tr>
      <% @chkl.rcel.keys[18..19].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.rcel[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>IDENTIFY/PRODUCE PREPOSITION CONCEPT</u>
        </td>
      </tr>
      <% @chkl.rcel.keys[20..21].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.rcel[k] %></td>
        </tr>
      <% end %>
      <tr>
        <td style="text-align:center; font-size:15px" colspan="2">
          <u>FOLLOW COMMAND</u>
        </td>
      </tr>
      <% @chkl.rcel.keys[22..25].each do |k| %>
        <tr>
          <td><%= k %></td>
          <td><%= @chkl.rcel[k] %></td>
        </tr>
      <% end %>
      <tr>
          <td>ADDTIONAL FOLLOW COMMAND</td>
          <td><%= @chkl.rcel["fcmd"] %></td>
        </tr>

    </tbody>
  </table>
  <!-- <b>RESULT/COMMENT</b><br>
  <%= @chkl.rslc %> -->

</div>